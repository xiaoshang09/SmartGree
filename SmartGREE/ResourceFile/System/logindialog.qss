}

/************系统按钮关闭******************/
QPushButton#pushButton_sysClose
{
        qproperty-text:"";
    qproperty-minimumSize: 60px 26px;
    qproperty-maximumSize: 60px 26px;
    border-image:url(:System/close_normal.png);
}
QPushButton#pushButton_sysClose:hover
{
    border-image:url(:System/close_hover.png);
}
QPushButton#pushButton_sysClose:pressed
{
    border-image:url(:System/close_press.png);
}
