}

/************系统按钮最大******************/
QPushButton#pushButton_sysMax
{
        qproperty-text:"";
    qproperty-minimumSize: 60px 26px;
    qproperty-maximumSize: 60px 26px;
    border-image:url(:System/max_normal.png);
}

QPushButton#pushButton_sysMax:hover
{
    border-image:url(:System/max_hover.png);
}
QPushButton#pushButton_sysMax:pressed
